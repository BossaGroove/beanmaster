  "name": "beanmaster_react",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.2.1",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "react": "^16.1.1",
    "react-bootstrap": "^0.31.5",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-svg-loader": "^2.1.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-manifest-plugin": "^1.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "bootstrap-sass": "^3.3.7",
    "change-case": "^3.0.1",
    "config": "^1.28.1",
    "fivebeans": "^1.5.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-csrf": "^3.0.6",
    "koa-helmet": "^3.3.0",
    "koa-logger": "^3.1.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.3.0",
    "koa-views": "^6.1.2",
    "lodash": "^4.17.4",
    "pug": "^2.0.0-rc.4",
    "require-all": "^2.2.0",
    "validator": "^9.1.2"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --port 4003 --progress --host 0.0.0.0",
    "start": "node scripts/start.js",
    "build": "npm run clean && webpack --hide-modules",
    "clean": "find ./public -type f | grep -v manifest.json | grep -v .gitignore | xargs rm -rf",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
