{
  "name": "beanmaster-react",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.0",
    "clean-css-loader": "^0.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-aftership": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "lodash-es": "^4.17.10",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.4",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.1",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "bootstrap-sass": "^3.3.7",
    "change-case": "^3.0.2",
    "debug": "^3.1.0",
    "fivebeans": "^1.5.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-csrf": "^3.0.6",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.5",
    "pug": "^2.0.3",
    "require-all": "^2.2.0",
    "validator": "^9.4.1"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --port 4003 --progress --host 0.0.0.0",
    "start": "NODE_ENV=development node ./server/bin/server.js",
    "prod": "NODE_ENV=production node ./server/bin/server.js",
    "build": "npm run clean && webpack --hide-modules",
    "clean": "find ./public -type f | grep -v manifest.json | grep -v .gitignore | xargs rm -rf",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --ext .jsx,.js ."
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
