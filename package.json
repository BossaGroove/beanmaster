{
  "name": "beanmaster",
  "version": "5.0.3",
  "bin": {
    "beanmaster": "server/bin/cli.js"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BossaGroove/beanmaster"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "autoprefixer": "^10.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-react-app": "^10.0.0",
    "babel-runtime": "^6.26.0",
    "bootstrap": "5.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^4.1.2",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-aftership": "^7.0.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^7.0.1",
    "jest": "^27.0.6",
    "lodash-es": "^4.17.21",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "postcss-loader": "^6.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.4",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1",
    "redux-form": "^8.3.7",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.50.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^4.0.2"
  },
  "dependencies": {
    "@koa/router": "^10.1.0",
    "app-root-path": "^3.0.0",
    "axios": "^0.28.0",
    "bluebird": "^3.7.2",
    "bunyan": "^1.8.15",
    "change-case": "^4.1.2",
    "commander": "^8.1.0",
    "daemonize2": "^0.4.2",
    "debug": "^4.3.2",
    "fivebeans": "^1.5.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-csrf": "^3.0.8",
    "koa-helmet": "^6.1.0",
    "koa-logger": "^3.2.1",
    "koa-response-time": "^2.1.0",
    "koa-static": "^5.0.0",
    "koa-views": "^7.0.1",
    "lodash": "^4.17.21",
    "pug": "^3.0.2",
    "require-all": "^3.0.0",
    "validator": "^13.7.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development node ./server/bin/server.js",
    "dev:browser": "NODE_ENV=development webpack serve --port 4000 --progress --host 0.0.0.0",
    "prod": "NODE_ENV=production node ./server/bin/server.js",
    "cli": "node ./server/bin/cli.js",
    "build": "npm run clean && npm run build:production",
    "build:production": "NODE_ENV=production webpack",
    "clean": "find ./public -type f | grep -v manifest.json | grep -v .gitignore | xargs rm -rf && touch ./public/tmp",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --ext .jsx,.js ."
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "license": "MIT"
}
